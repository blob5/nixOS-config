    "format-icons": ["Û∞§Ø","Û∞§ü","Û∞§¢","Û∞§•","Û∞§®"]
},


  "tray": {
    "icon-size": 16,
    "rotate": 0,
    "spacing": 3
  },

  "group/expand": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 600,
      "children-class": "not-power",
      "transition-to-left": true
    },
    "modules": ["custom/menu", "custom/spacer", "tray"]
  },

  "custom/menu": {
    "format": "Û∞ÖÉ",
    "rotate": 90
